  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "! name: scope_reduction.1\n",
    "! type: F-free\n",
    "! version: omp_5.1\n",
    "subroutine do_work(n, a, s)\n",
    "   implicit none\n",
    "   integer n, i\n",
    "   real a(*), s, loc_s\n",
    "   integer, save :: nthrs\n",
    "\n",
    "   loc_s = 0.0                ! local sum\n",
    "   !$omp do\n",
    "      do i = 1, n\n",
    "         loc_s = loc_s + a(i)\n",
    "      end do\n",
    "   !$omp single\n",
    "      s = 0.0                 ! total sum\n",
    "      nthrs = 0\n",
    "   !$omp end single\n",
    "   !$omp scope reduction(+:s,nthrs)\n",
    "      s = s + loc_s\n",
    "      nthrs = nthrs + 1\n",
    "   !$omp end scope\n",
    "   !$omp masked\n",
    "      print *, \"total sum = \", s, \", nthrs = \", nthrs\n",
    "   !$omp end masked\n",
    "end subroutine\n",
    "\n",
    "function work(n, a) result(s)\n",
    "   implicit none\n",
    "   integer n\n",
    "   real a(*), s\n",
    "\n",
    "   !$omp parallel\n",
    "      call do_work(n, a, s)\n",
    "   !$omp end parallel\n",
    "end function\n",
    "\n"
   ]
