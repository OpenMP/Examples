    "\n",
    "! name: reduction.7\n",
    "! type: F-free\n",
    "program array_red\n",
    "\n",
    "  integer,parameter :: n=100\n",
    "  integer           :: j\n",
    "  real              :: a(n), b(n,n)\n",
    "\n",
    "  call init(n,b)\n",
    "\n",
    "  a(:) = 0.0e0\n",
    "\n",
    "  !$omp parallel do reduction(+:a)\n",
    "  do j = 1, n\n",
    "     a(:) = a(:) + b(:,j)\n",
    "  end do\n",
    "\n",
    "  print*, \" a(1) a(n): \", a(1), a(n)\n",
    "\n",
    "end program\n",
