    "\n",
    "! name: reduction.2\n",
    "! type: F-free\n",
    "  SUBROUTINE REDUCTION2(A, B, C, D, X, Y, N)\n",
    "    REAL :: X(*), A, D\n",
    "    INTEGER :: Y(*), N, B, C\n",
    "    REAL :: A_P, D_P\n",
    "    INTEGER :: I, B_P, C_P\n",
    "    A = 0\n",
    "    B = 0\n",
    "    C = Y(1)\n",
    "    D = X(1)\n",
    "    !$OMP PARALLEL SHARED(X, Y, A, B, C, D, N) &\n",
    "    !$OMP&         PRIVATE(A_P, B_P, C_P, D_P)\n",
    "      A_P = 0.0\n",
    "      B_P = 0\n",
    "      C_P = HUGE(C_P)\n",
    "      D_P = -HUGE(D_P)\n",
    "      !$OMP DO PRIVATE(I)\n",
    "      DO I=1,N\n",
    "        A_P = A_P + X(I)\n",
    "        B_P = IEOR(B_P, Y(I))\n",
    "        C_P = MIN(C_P, Y(I))\n",
    "        IF (D_P < X(I)) D_P = X(I)\n",
    "      END DO\n",
    "      !$OMP CRITICAL\n",
    "        A = A + A_P\n",
    "        B = IEOR(B, B_P)\n",
    "        C = MIN(C, C_P)\n",
    "        D = MAX(D, D_P)\n",
    "      !$OMP END CRITICAL\n",
    "    !$OMP END PARALLEL\n",
    "  END SUBROUTINE REDUCTION2\n",
    "\n"
