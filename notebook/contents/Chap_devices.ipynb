   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "! name: teams.5\n",
    "! type: F-free\n",
    "! version: omp_4.0\n",
    "subroutine vec_mult(p, v1, v2, N)\n",
    "   real    ::  p(N), v1(N), v2(N)\n",
    "   integer ::  i\n",
    "   call init(v1, v2, N)\n",
    "   !$omp target teams map(to: v1, v2) map(from: p)\n",
    "      !$omp distribute simd\n",
    "         do i=1,N\n",
    "            p(i) = v1(i) * v2(i)\n",
    "         end do\n",
    "   !$omp end target teams\n",
    "   call output(p, N)\n",
    "end subroutine\n",
    "\n"
   ]
