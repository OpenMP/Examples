    "* type: C\n",
    "*/\n",
    "void work(int i, int j) {}\n",
    "void work1(int i, int n)\n",
    "{\n",
    "  int j;\n",
    "/* incorrect nesting of loop regions */\n",
    "  #pragma omp for\n",
    "    for (j=0; j<n; j++)\n",
    "      work(i, j);\n",
    "}\n",
    "\n",
    "void wrong2(int n)\n",
    "{\n",
    "  #pragma omp parallel default(shared)\n",
    "  {\n",
    "    int i;\n",
    "    #pragma omp for\n",
    "      for (i=0; i<n; i++)\n",
    "         work1(i, n);\n",
    "  }\n",
    "}\n",
    "\n"
