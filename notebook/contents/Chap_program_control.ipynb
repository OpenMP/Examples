    "* name: nesting_restrict.4\n",
    "* type: C\n",
    "*/\n",
    "void work(int i, int j) {}\n",
    "void wrong4(int n)\n",
    "{\n",
    "\n",
    "  #pragma omp parallel default(shared)\n",
    "  {\n",
    "    int i;\n",
    "    #pragma omp for\n",
    "      for (i=0; i<n; i++) {\n",
    "        work(i, 0);\n",
    "/* incorrect nesting of barrier region in a loop region */\n",
    "        #pragma omp barrier\n",
    "        work(i, 1);\n",
    "      }\n",
    "  }\n",
