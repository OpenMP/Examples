    "* type: C\n",
    "*/\n",
    "void work(int i, int j) {}\n",
    "void wrong3(int n)\n",
    "{\n",
    "  #pragma omp parallel default(shared)\n",
    "  {\n",
    "    int i;\n",
    "    #pragma omp for\n",
    "      for (i=0; i<n; i++) {\n",
    "/* incorrect nesting of regions */\n",
    "        #pragma omp single\n",
    "          work(i, 0);\n",
    "      }\n",
    "  }\n",
