   "source": [
    "//%compiler: clang\n",
 "//%cflags: -fopenmp\n",
 "\n",
    "/*\n",
    "* name: SIMD.7\n",
    "* type: C\n",
    "* version: omp_4.0\n",
    "*/\n",
    "#include <stdio.h>\n",
    "#include <stdlib.h>\n",
    "\n",
    "#define N 45\n",
    "int a[N], b[N], c[N];\n",
    "\n",
    "#pragma omp declare simd inbranch\n",
    "int fib( int n )\n",
    "{\n",
    "   if (n <= 1)\n",
    "      return n;\n",
    "   else {\n",
    "      return fib(n-1) + fib(n-2);\n",
    "   }\n",
    "}\n",
    "\n",
    "int main(void)\n",
    "{\n",
