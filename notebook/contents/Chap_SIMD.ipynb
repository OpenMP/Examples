    "\n",
    "   #pragma omp simd\n",
    "   for (i=0; i < N; i++) {\n",
    "      a[i] = fib(b[i]);\n",
    "   }\n",
    "   printf(\"Done a[%d] = %d\\n\", N-1, a[N-1]);\n",
    "   return 0;\n",
    "}\n",
    "\n"
