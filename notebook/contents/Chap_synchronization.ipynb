    "\n",
    "! name: ordered.3\n",
    "! type: F-fixed\n",
    "      SUBROUTINE ORDERED_GOOD(N)\n",
    "      INTEGER N\n",
    "\n",
    "!$OMP   DO ORDERED\n",
    "        DO I = 1,N\n",
    "          IF (I <= 10) THEN\n",
    "!$OMP       ORDERED\n",
