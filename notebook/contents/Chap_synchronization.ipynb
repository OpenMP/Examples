   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "//%compiler: clang\n",
 "//%cflags: -fopenmp\n",
 "\n",
    "/*\n",
    "* name: ordered.3\n",
    "* type: C\n",
    "*/\n",
    "void work(int i) {}\n",
    "void ordered_good(int n)\n",
    "{\n",
    "  int i;\n",
    "#pragma omp for ordered\n",
    "  for (i=0; i<n; i++) {\n",
    "    if (i <= 10) {\n",
    "      #pragma omp ordered\n",
    "         work(i);\n",
    "    }\n",
    "    if (i > 10) {\n",
    "      #pragma omp ordered\n",
    "        work(i+1);\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n"
   ]
