    "!$OMP       END ORDERED\n",
    "          ENDIF\n",
    "\n",
    "          IF (I > 10) THEN\n",
    "!$OMP       ORDERED\n",
    "              CALL WORK(I+1)\n",
    "!$OMP       END ORDERED\n",
    "          ENDIF\n",
    "        ENDDO\n",
    "      END SUBROUTINE ORDERED_GOOD\n",
