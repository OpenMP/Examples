    "         }\n",
    "         #pragma omp flush // or with acq_rel or acquire clause\n",
    "         printf(\"x = %d\\n\", x);  // always \"x = 10\"\n",
    "      }\n",
    "   }\n",
    "   return 0;\n",
    "}\n",
    "\n"
   ]
