    "*/\n",
    "void work(int i) {}\n",
    "\n",
    "void ordered_wrong(int n)\n",
    "{\n",
    "  int i;\n",
    "  #pragma omp for ordered\n",
    "  for (i=0; i<n; i++) {\n",
    "/* incorrect because an iteration may not execute more than one\n",
    "   ordered region */\n",
    "    #pragma omp ordered\n",
    "      work(i);\n",
    "    #pragma omp ordered\n",
    "      work(i+1);\n",
    "  }\n",
