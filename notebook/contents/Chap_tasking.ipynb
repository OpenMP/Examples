   "outputs": [],
   "source": [
    "\n",
    "! name: task_dep.10\n",
    "! type: F-free\n",
    "! version:    omp_5.0\n",
    "subroutine foo\n",
    "   integer :: a,b,c\n",
    "   c = 0\n",
    "   !$omp parallel\n",
    "   !$omp single\n",
    "      !$omp task depend(out: a)\n",
    "         a = longTaskA()\n",
    "      !$omp end task\n",
    "      !$omp task depend(out: b)\n",
    "         b = shortTaskB()\n",
