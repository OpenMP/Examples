    "! type: F-free\n",
    "! version:    omp_3.1\n",
    "subroutine foo()\n",
    "  integer :: x\n",
    "  x = 2\n",
    "!$omp task shared(x) mergeable\n",
    "  x = x + 1\n",
    "!$omp end task\n",
    "!$omp taskwait\n",
    "  print *, x     ! prints 3\n",
    "end subroutine\n",
    "\n"
   ]
