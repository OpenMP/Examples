   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "! name: task_dep.1\n",
    "! type: F-free\n",
    "! version:    omp_4.0\n",
    "program example\n",
    "   integer :: x\n",
    "   x = 1\n",
    "   !$omp parallel\n",
    "   !$omp single\n",
    "      !$omp task shared(x) depend(out: x)\n",
    "         x = 2\n",
    "      !$omp end task\n",
    "      !$omp task shared(x) depend(in: x)\n",
    "         print*, \"x = \", x\n",
    "      !$omp end task\n",
    "   !$omp end single\n",
    "   !$omp end parallel\n",
    "end program\n",
    "\n"
   ]
