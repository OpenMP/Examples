    "      include 'omp_lib.h'\n",
    "      integer (kind=omp_lock_kind) lock\n",
    "      integer i\n",
    "\n",
    "      contains\n",
    "\n",
    "      subroutine work\n",
    "      call omp_init_lock(lock)\n",
