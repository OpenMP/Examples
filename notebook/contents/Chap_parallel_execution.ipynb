    "!$OMP SECTION\n",
    "        CALL XAXIS()\n",
    "!$OMP SECTION\n",
    "        CALL YAXIS()\n",
    "\n",
    "!$OMP SECTION\n",
    "        CALL ZAXIS()\n",
    "\n",
    "!$OMP END PARALLEL SECTIONS\n",
    "      END SUBROUTINE SECT_EXAMPLE\n",
    "\n"
