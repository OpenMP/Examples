    "\n",
    "! name: collapse.3\n",
    "! type: F-fixed\n",
    "! version: omp_3.0\n",
    "      program test\n",
    "      include 'omp_lib.h'\n",
    "!$omp parallel num_threads(2)\n",
    "!$omp do collapse(2) ordered private(j,k) schedule(static,3)\n",
    "      do k = 1,3\n",
    "        do j = 1,2\n",
    "!$omp ordered\n",
    "          print *, omp_get_thread_num(), k, j\n",
    "!$omp end ordered\n",
    "          call work(a,j,k)\n",
    "        enddo\n",
    "      enddo\n",
    "!$omp end do\n",
    "!$omp end parallel\n",
    "      end program test\n",
    "\n"
   ]
